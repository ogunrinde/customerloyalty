{
  "name": "LoyaltyTrackingSystem",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=asset/img/icon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=asset/img/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Customer Tracking\"",
    "package-linux" : "electron-packager . --overwrite --platform=linux --arch=x64 --icon=asset/img/icon.ico --prune=true --out=release-builds"
  },
  "author": "Ogunrinde Omotayo",
  "license": "ISC",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-packager": "^14.0.3"
  }
}
